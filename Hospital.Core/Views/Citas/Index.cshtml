@{
    ViewData["Title"] = "Index";

}
<div class=" p-3">
    <div class="d-flex justify-content-between pt-4">
        <div class="p-2">
            <h2 class="text-primary">Mantenimiento Citas</h2>
        </div>
        <div class="p-2">
            <a style="text-decoration:none;" class="btn btn-info" asp-controller="Citas"
               asp-action="Create">
                Agregar Cita
            </a>
        </div>
    </div>
    <br />
    <table class="table table-bordered table-striped " id="tblData" style="width:100%;">
        <thead>
            <tr>
                <th>
                    ID
                </th>
                <th>
                    Nombre paciente
                </th>
                <th>
                    Servicio
                </th>
                <th>
                    Fecha agendada
                </th>
                <th>
                    Horario
                </th>
                <th>
                    Estado
                </th>
                <th>
                    @*buttons*@
                </th>
            </tr>
        </thead>
    </table>
</div>
@section Scripts {
    <script src="~/js/Citas/Index.js"></script>
}


